<apex:page standardController="Consultant__c">
<apex:form >
  <apex:pageBlock >
      <apex:pageBlockButtons >
          <apex:commandButton action="{!save}" value="Register"/>
      </apex:pageBlockButtons>
      <apex:pageBlockSection title="Information">
          <apex:inputField value="{!Consultant__c.Full_Name__c}"/>
          <apex:inputField value="{!Consultant__c.Email__c}"/>
          <apex:inputSecret value="{!Consultant__c.password__c}"/>
          <apex:inputField value="{!Consultant__c.Mobile_Number__c}"/>
          <apex:inputField value="{!Consultant__c.Date_of_birth__c}"/>
          <apex:inputField value="{!Consultant__c.Address__c}"/>
      </apex:pageBlockSection>
  </apex:pageBlock>
  </apex:form>
</apex:page>